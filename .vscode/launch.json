  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": ["cat"],
      "program": "${workspaceFolder}/source/cli.tsx",
      "preLaunchTask": "build-cli",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      
    }
  ]
}